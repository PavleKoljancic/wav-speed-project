    "version": "0.2.0",
    "configurations": [{
        "name": "(gdb) Launch",
        "type": "cppdbg",
        "request": "launch",
        "program": "${workspaceRoot}/bin/Main",
        "args": ["-i", "/home/pavle/Desktop/FAKS/3/Projekat 2/2000.wav", "-o", "\"res.wav\"", "-s", "1.75", "-b" ,"100" ,"-t" ,"12" ,"-x", "1"],
        "stopAtEntry": false,
        "cwd": "${workspaceRoot}",
        "environment": [],
        "externalConsole": false,
        "MIMode": "gdb",
        "setupCommands": [
            {
                "description": "Enable pretty-printing for gdb",
                "text": "-enable-pretty-printing",
                "ignoreFailures": true
